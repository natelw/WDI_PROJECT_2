<div class="container">
  <div class="row">
    <div class="col s12">
      <ul class="tabs">


        <li class="tab col s3"><a href="#char1"><%= sheet.charName %></a></li>
        <li class="tab col s3"><a href="#char2">name 2</a></li>
        <li class="tab col s3"><a href="#char3">name 3</a></li>
        <li class="tab col s3"><a href="#char4">name 4</a></li>
      </ul>
    </div>
    <div id="char1" class="col s12">
      <%= sheet.charName %>
      <%= sheet.charClass %>
      <%= sheet.charLevel %>
      <%= sheet.charRace %>
    </div>
    <div id="char2" class="col s12">Test 2</div>
    <div id="char3" class="col s12">Test 3</div>
    <div id="char4" class="col s12">Test 4</div>
  </div>
</div>
<div class="fixed-action-btn">
  <a class="btn-floating btn-large red">
    <i class="large material-icons">mode_edit</i>
  </a>
  <ul>
    <li><a class="btn-floating red"><i class="material-icons">delete_forever</i></a></li>
    <li><a class="btn-floating yellow darken-1" href="/sheets/<%= sheet.id %>/edit">
      <i class="material-icons">edit</i></a></li>
    <li><a class="btn-floating blue" href="/sheets" ><i class="material-icons">arrow_back</i></a></li>
    </ul>
  </div>
